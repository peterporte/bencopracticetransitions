        <div class="panel-body panel-primary">
            <div id="iosAlertPlaceholder"></div>
            <div><partial name="_Alert" /></div>
            <div class="panel panel-default" style="width: 100%">
                <div class="panel-heading">
                    <div class="panel-title">
                        <h4>Thank You</h4>
                    </div>
                </div>
                <div class="panel-body">
                    Thank you for choosing Practice Transitions from Benco Dental!
                    Someone will be contacting you shortly!
                    <div style="padding-top: 50px;">
                        @Html.ActionLink("Back to Homepage", "Index", "Home", null, new { @class = "btn btn-primary", style = "max-width: 150px;" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="col-md-8 col-md-offset-2 text-center" style="padding: 50px 0 100px 0;">
    <h2></h2>
