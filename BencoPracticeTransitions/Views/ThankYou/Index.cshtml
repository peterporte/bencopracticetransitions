@{
    ViewData["Title"] = "Thank You";
    Layout = "~/Views/Shared/_layout.cshtml";
}


<div class="row" style="padding-bottom: 20px;">
    <div class="col-xs-10 col-xs-offset-1 col-sm-6 col-sm-offset-1 col-md-5 col-md-offset-1 col-lg-4 col-lg-offset-1">
        <h1>Practice Transitions</h1>
        <span>by <img src="~/images/benco_logo_no_tagline_225.png" style="transform: scale(0.67); transform-origin: 0;" alt="logo" /></span>
    </div>
</div>
<br />
<div class="row">
